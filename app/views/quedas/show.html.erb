<p id="notice"><%= notice %></p>

<p>
  <strong>Imei:</strong>
  <%= @queda.imei %>
</p>

<p>
  <strong>Email:</strong>
  <%= @queda.email %>
</p>

<p>
  <strong>Classificação:</strong>
  <%= @queda.tipo_queda %>
</p>

<p>
  <strong>Timestamp queda:</strong>
  <%= if @queda.tempo_queda.blank? then " - " else Time.at(Integer(@queda.tempo_queda)) end %>
</p>

<p>
  <strong>Timestamp recebimento:</strong>
  <%= if @queda.tempo_recebimento.blank? then " - " else Time.at(Integer(@queda.tempo_recebimento)) end %>
</p>

<p>
  <strong>Amostra:</strong>
  <%= @queda.amostra %>
</p>

<p>
  <strong>GPS:</strong>
  <%= @queda.locationX %> - 
  <%= @queda.locationY %>
</p>

<%= link_to 'Edit', edit_queda_path(@queda) %> |
<%= link_to 'Back', quedas_path %>
